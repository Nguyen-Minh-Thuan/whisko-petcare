				{
					"name": "Register User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    const response = pm.response.json();",
									"    if (response.data && response.data.token) {",
									"        pm.collectionVariables.set('jwt_token', response.data.token);",
									"        console.log('JWT token saved:', response.data.token);",
									"    }",
									"    if (response.data && response.data.user_id) {",
									"        pm.collectionVariables.set('user_id', response.data.user_id);",
									"        console.log('User ID saved:', response.data.user_id);",
									"    }",
									"    pm.test('User registered successfully', () => {",
									"        pm.expect(response.success).to.be.true;",
									"    });",
									"}"
								],
